{
  "name": "bucklescript-tea-showcase",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["bucklescript-tea", "bs-fetch"],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "ppx-flags": ["graphql_ppx/ppx"],
  "warnings": {
    "error": "+101+8"
  },
  "refmt": 3,
  "bsc-flags": ["-bs-super-errors", "-bs-g"]
}
